<h1>ログイン</h1>
<div>
    <%= form_with(url: login_path, scope: :session, local: true, data: { turbo: false }) do |f| %>
        <div>
            <%= f.label :login_id %>
            <%= f.text_field :login_id %>
        </div>
        <div>
            <%= f.label :password %>
            <%= f.password_field :password %>
        </div>
        <div>
            <%= f.submit 'Login' %>
        </div>
    <% end %>
</div>
